{
  "parser": "babel-eslint",
  "rules": {
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "no-unused-vars": [
      2,
      {
        "vars": "all",
        "args": "after-used",
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ],
    "semi": [
      2,
      "never"
    ],
    "no-console": 0,
    "id-length": [
      2,
      {
        "min": 2,
        "exceptions": [
          "i",
          "e"
        ],
      }
    ],
  },
  "env": {
    "es6": true,
    "node": true
  },
  "plugins": []
}
